<?xml version="1.0"?>
<kml:kml xmlns:kml="http://www.opengis.net/kml/2.2" xmlns:kmlpipe="https://github.com/dechamps/kmlpipe">
  <kml:Document>
    <kml:Folder>
      <kml:name>Source Folder</kml:name>
      <kml:Placemark>
        <kml:name>Source Placemark 1</kml:name>
        <kmlpipe:LinkSet name="Link Set">
          <kmlpipe:Link place-id="dest1"/>
          <kmlpipe:Link place-id="dest2"/>
        </kmlpipe:LinkSet>
      </kml:Placemark>
      <kml:Placemark>
        <kml:name>Source Placemark 2</kml:name>
        <kmlpipe:LinkSet name="Link Set">
          <kmlpipe:Link place-id="dest1"/>
          <kmlpipe:Link place-id="dest2"/>
        </kmlpipe:LinkSet>
      </kml:Placemark>
    </kml:Folder>
    <kml:Folder>
      <kml:name>Destination Folder</kml:name>
      <kml:Placemark>
        <kmlpipe:Place place-id="dest1"/>
        <kml:name>Destination Placemark 1</kml:name>
      </kml:Placemark>
      <kml:Placemark>
        <kmlpipe:Place place-id="dest2"/>
        <kml:name>Destination Placemark 2</kml:name>
      </kml:Placemark>
    </kml:Folder>
  </kml:Document>
</kml:kml>
